package system.main.navigation;

public class Node {
	
	boolean checked;
	
	Node parent;
	
	int x;
	int y;
	
	public Node(int X, int Y) {
		x = X;
		y = Y;
		
		checked = false;
	}
	
	public void setParent(Node p) {
		parent = p;
	}
	
	public int[][] getChildren() {
		for(int y = -1; y <= 1; y++) {
			for(int y = -1; y <= 1; y++) {
				
			}
		}
		return null;
	}
}
